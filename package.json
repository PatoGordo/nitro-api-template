{
  "scripts": {
    "dev": "nitropack dev",
    "build": "nitropack build",
    "preview": "node .output/server/index.mjs",
    "test": "jest",
    "lint": "eslint --fix --ext .js,.ts --ignore-path .eslintignore .",
    "format": "prettier --write --check .",
    "lint-staged": "yarn lint && yarn format",
    "prisma:inline": "cp ./node_modules/.prisma/client/*.js ./node_modules/@prisma/client",
    "prisma:migrate": "prisma migrate deploy && npm run prisma:inline",
    "prisma:migrate:dev": "prisma migrate dev && npm run prisma:inline",
    "prisma:generate": "prisma generate && npm run prisma:inline",
    "postinstall": "yarn prisma:generate",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@swc/core": "^1.3.28",
    "@swc/jest": "^0.2.24",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "nitropack": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "add": "^2.0.6",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "i18next": "^22.4.9",
    "jsonwebtoken": "^9.0.0",
    "limiter": "^2.1.0",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.0",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "yarn": "^1.22.19",
    "zod-i18n-map": "^2.2.1"
  }
}
